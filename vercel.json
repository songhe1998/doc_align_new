{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py/$1"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py/openapi.json"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py/docs"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/index.html"
    }
  ]
}